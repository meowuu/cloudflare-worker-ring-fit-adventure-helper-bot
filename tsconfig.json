{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "lib": ["ESNext", "WebWorker"],
    "types": [
      "@cloudflare/workers-types"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
